{
  "name": "nano-binary-search",
  "description": "Binary search for JavaScript done right.",
  "version": "1.0.5",
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    }
  },
  "scripts": {
    "start": "tape6-server --trace",
    "test": "tape6 --flags FO",
    "test:bun": "tape6-bun --flags FO",
    "test:deno": "deno run -A `tape6-runner main` --flags FO",
    "test:deno-original": "tape6-deno --flags FO",
    "ts-check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uhop/nano-binary-search.git"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/uhop"
  },
  "bugs": {
    "url": "https://github.com/uhop/nano-binary-search/issues"
  },
  "homepage": "https://github.com/uhop/nano-binary-search#readme",
  "keywords": [
    "binary search",
    "algorithm"
  ],
  "author": "Eugene Lazutkin <eugene.lazutkin@gmail.com> (https://www.lazutkin.com/)",
  "license": "BSD-3-Clause",
  "files": [
    "index.*",
    "LICENSE",
    "README.md"
  ],
  "tape6": {
    "tests": [
      "tests/test-*.*js"
    ],
    "importmap": {
      "imports": {
        "tape-six": "/node_modules/tape-six/index.js",
        "nano-binary-search": "/index.js"
      }
    }
  },
  "devDependencies": {
    "tape-six": "^0.12.1",
    "typescript": "^5.6.2"
  }
}
